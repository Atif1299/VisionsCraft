<%- include('partials/header') %>
        <!-- Hero Section -->
        <section class="services-hero">
            <div class="container">
                <div class="services-hero-content">
                    <h1 class="animate-text">
                        Our
                        <span class="gradient-text">AI Services</span>
                    </h1>
                    <p class="animate-text-delay">Empowering your business with innovative, practical, and ethical AI solutions.</p>
                </div>
            </div>
            <div id="particles-background" class="particles-container"></div>
        </section>
        <!-- Services Grid -->
        <section class="services" id="services">
            <div class="container">
                <div class="services-grid">
                    <div class="service-card" data-aos="fade-up">
                        <div class="service-icon">
                            <i class="fas fa-robot"></i>
                        </div>
                        <h3>AI Strategy Consulting</h3>
                        <p>Strategic guidance to integrate AI into your business operations for maximum impact and ROI.</p>
                    </div>
                    <div class="service-card" data-aos="fade-up" data-aos-delay="100">
                        <div class="service-icon">
                            <i class="fas fa-brain"></i>
                        </div>
                        <h3>Machine Learning Solutions</h3>
                        <p>Custom ML models that learn from your data to automate processes and uncover valuable insights.</p>
                    </div>
                    <div class="service-card" data-aos="fade-up" data-aos-delay="200">
                        <div class="service-icon">
                            <i class="fas fa-comments"></i>
                        </div>
                        <h3>Conversational AI</h3>
                        <p>Intelligent chatbots and virtual assistants that enhance customer experience and streamline support.</p>
                    </div>
                    <div class="service-card" data-aos="fade-up" data-aos-delay="300">
                        <div class="service-icon">
                            <i class="fas fa-chart-line"></i>
                        </div>
                        <h3>Predictive Analytics</h3>
                        <p>Advanced analytics that forecast trends, identify opportunities, and mitigate risks before they emerge.</p>
                    </div>
                    <div class="service-card" data-aos="fade-up" data-aos-delay="400">
                        <div class="service-icon">
                            <i class="fas fa-eye"></i>
                        </div>
                        <h3>Computer Vision</h3>
                        <p>Image and video analysis solutions for automation, quality control, and enhanced insights.</p>
                    </div>
                    <div class="service-card" data-aos="fade-up" data-aos-delay="500">
                        <div class="service-icon">
                            <i class="fas fa-language"></i>
                        </div>
                        <h3>Natural Language Processing</h3>
                        <p>Text and speech processing to extract meaning, automate tasks, and improve communication.</p>
                    </div>
                </div>
            </div>
        </section>
        <section class="partners-section">
            <div class="container">
                <div class="section-heading text-center">
                    <h2>Tech Stack & Tools</h2>
                    <p>Working with Top Plateforms </p>
